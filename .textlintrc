{
  "filters": {
    "comments": true
  },
  "rules": {
    "preset-ja-spacing": {
      "ja-space-between-half-and-full-width": {
        "space": [
          "alphabets",
          "numbers",
          "punctuation"
        ]
      },
      "ja-no-space-around-parentheses": false,
      "ja-space-around-link": {
        "before": true,
        "after": true
      }
    },
    "preset-ja-technical-writing": {
      "no-exclamation-question-mark": false,
      "no-mix-dearu-desumasu": {
        "preferInHeader": "ですます",
        "preferInBody": "ですます",
        "preferInList": "ですます"
      },
      "sentence-length": {
        "max": 120
      },
      "max-kanji-continuous-len": {
        "max": 8
      },
      "ja-no-mixed-period": {
        "periodMark": "。",
        "allowPeriodMarks": [
          "\"",
          "."
        ],
        "allowEmojiAtEnd": false,
        "forceAppendPeriod": false
      }
    },
    "ja-no-orthographic-variants": true,
    "@textlint-ja/no-synonyms": {
      "allows": [
        "運用",
        "開催",
        "完了",
        "規約",
        "許諾",
        "計画",
        "行動",
        "実施",
        "出願",
        "使用",
        "申請",
        "テンプレート",
        "特許権",
        "名前",
        "フォーム",
        "方式",
        "メソッド",
        "利用",
        "ルール",
        "ロール"
      ],
      "preferWords": [
        "アダプター",
        "アンダーバー",
        "イラストレーター",
